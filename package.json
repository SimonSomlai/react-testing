{
  "name": "react-testing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "es6-promise": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "mocha": "^3.3.0",
    "node-inspector": "^1.1.1",
    "react-addons-test-utils": "^15.5.1",
    "react-scripts": "0.9.5",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "//": "react-scripts test --env=jsdom",
    "test": "mocha --require ./test_setup.js --compilers babel-core/register ./test/*test.js",
    "test:debug": "mocha --require ./test_setup.js --compilers babel-core/register --debug-brk ./test/*test.js",
    "eject": "react-scripts eject"
